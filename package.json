{
  "name": "rpcapps",
  "version": "1.0.0",
  "description": "Simple Discord RPC Client ~nya",
  "build": {
    "appId": "NyanRPC",
    "productName": "NyanRPC",
    "copyright": "Copyright Â© year ${author}",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "assets/img/icon.ico",
      "legalTrademarks": "GitHub"
    },
    "nsis": {
      "installerIcon": "assets/img/icon.ico",
      "uninstallerIcon": "assets/img/exe.ico",
      "uninstallDisplayName": "${productName} ${version}",
      "license": "license.txt",
      "multiLanguageInstaller": false,
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "runAfterFinish": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "menuCategory": false
    }
  },
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "npm i && npm run download && npm run build && pause",
    "start": "electron index.cjs",
    "download": "npm i -g electron electron-builder",
    "buildPortable": "electron-packager . nyan --overwrite --asar=true --platform=win32 --arch=x64 --electronVersion=18.0.4  --icon=assets/img/icon.ico --prune=true --out=release-builds --version-string.CompanyName=Aiko  --version-string.FileDescription=A Discord RPC product  --version-string.ProductName=\"NyanRPC\"",
    "build": "electron-builder",
    "build:css": "tailwindcss -i ./src/Views/input.css -o ./src/Views/output.css --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ascii-table": "^0.0.9",
    "body-parser": "^1.20.0",
    "chalk": "^5.0.1",
    "cors": "^2.8.5",
    "discord-rpc": "^4.0.1",
    "discord.js-selfbot-v13": "^2.4.2",
    "express": "^4.18.1",
    "path": "^0.12.7",
    "socket.io": "^4.5.1",
    "tailwind-scrollbar": "^1.3.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "electron": "^19.0.8",
    "tailwindcss": "^3.0.24"
  }
}
